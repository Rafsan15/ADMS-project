@model FMS_Model.RequestedMemberModel

@{
    ViewBag.Title = "RequestedMember";
    Layout = "~/Views/Shared/AfterLoginOwner.cshtml";
}


        <div class="container"><h1>Requested Members  </h1></div>
        <div id="exTab1" class="container">
            <ul class="nav nav-pills">
                <li class="active">
                    <!--	<button id="1a" class="btn btn-primary" type="button" data-toggle="tab" onclick="displayDate()"> Overview </button>-->
                    <a data-toggle="tab" class="tabLink" onclick="TabClick('1a', displayDate)"><h4>All Requests</h4></a>
                </li>
                <li>
                    <!--<button  id="2a" class="btn btn-primary" type="button" data-toggle="tab" onclick="displayDate2()"> Overview2 </button>-->
                    <a data-toggle="tab" class="tabLink" onclick="TabClick('2a', displayDate2)"><h4>Top 5</h4></a>
                </li>
                <li>
                    <a class="tabLink" data-toggle="tab" onclick="TabClick('3a', displayDate)"><h4>Top 10</h4></a>
                </li>
                <li>
                    <a class="tabLink" data-toggle="tab" onclick="TabClick('4a', displayDate3)"><h4>On Time</h4></a>
                </li>
            </ul>

            <div class="tab-content clearfix">
                <div class="tab-pane active tabLink" id="1a">
                    <div class="container">

                        <div class="page-header">
                        </div>
                        <div class="row">
                            <div class="col-md-2"></div>
                            <div class="col-md-8">
                                <div class="panel panel-primary">
                                    <div class="panel-heading">
                                        <h3 class="panel-title">Requested Member List</h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <h4>@Model.ProjectName</h4>
                                                <p>@Model.Description</p>
                                            </div>
                                            <div class="col-md-6"></div>
                                            <div class="col-md-3"></div>
                                        </div>
                                        <h3>Requested Members</h3>
                                        <table class="table table-striped table-hover ">

                                            <tr>

                                                <th>Name</th>
                                                <th>Details</th>
                                                <th>Response</th>

                                            </tr>

                                            @foreach (var p in Model.UserInfo)
                                            {
                                                <tr>
                                                    <div class="row">

                                                        <th>
                                                            <a href="ProfileWorker.html"><h5>@p.FristName @p.LastName</h5></a>
                                                            <h6>Hi, I am so interest to your job. </h6>
                                                        </th>

                                                        <th>

                                                            <h5>$300 USD in 3 days</h5>
                                                            <span class="fa fa-star checked"></span>
                                                            <span class="fa fa-star checked"></span>
                                                            <span class="fa fa-star checked"></span>
                                                            <span class="fa fa-star checked"></span>
                                                            <span class="fa fa-star"></span>
                                                            5.0
                                                        </th>
                                                        <th>
                                                            @using (Html.BeginForm("RequestedMember", "ProjectPost"))
                                                            {
                                                                <input type="hidden" name="PostId" value="@Model.PostId" />
                                                                 <input type="hidden" name="UserId" value="@p.UserId" />


                                                                <div class="col-lg-3 col-lg-offset-2">
                                                                    
                                                                    <input type="submit" id="@p.UserId" onclick="" value="Hire" class="btn btn-primary"/>
                                                                   
                                                                </div>
                                                            }

                                                        </th>
                                                    </div>
                                                    <hr />
                                                </tr>

                                            }
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2"></div>
                        </div>


                    </div>

                </div>
                <div class="tab-pane tabLink" id="2a">
                    <div class="container">

                        <div class="page-header">
                        </div>
                        <div class="row">
                            <div class="col-md-2"></div>
                            <div class="col-md-8">
                                <div class="panel panel-primary">
                                    <div class="panel-heading">
                                        <h3 class="panel-title">Requested Member List</h3>
                                    </div>
                                    <div class="panel-body">

                                        <h3>Top 5</h3>
                                        <div class="row">
                                            <table class="table table-striped table-hover ">

                                                <tr>

                                                    <th>Name</th>
                                                    <th>Details</th>
                                                    <th>Response</th>
                                                    <th>View</th>

                                                </tr>

                                                @foreach (var p in Model.UserInfo)
                                                {
                                                    <tr>
                                                        <div class="row">

                                                            <th>
                                                                <a href="ProfileWorker.html"><h5>@p.FristName @p.LastName</h5></a>
                                                                <h6>Hi, I am so interest to your job. </h6>
                                                            </th>


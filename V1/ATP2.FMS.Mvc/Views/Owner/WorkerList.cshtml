@model FMS_Model.RatingWorkerModel
@{
    ViewBag.Title = "WorkerList";
    Layout = "~/Views/Shared/AfterLoginOwner.cshtml";
}
@for (int i = 0; i < @Model.UserId.Count; i++)
{
    <a href="@Url.Action("RatingWorkers","Owner")/@Model.UserId[i]" class="btn btn-success" type="button">@Model.Name[i]</a>
    <br/>

}

<a href="@Url.Action("DeletePost","Owner")/@Model.PostId" class="btn btn-success" type="button">Done</a>

